{
  "env": {
    "node": true,
    "commonjs": true
  },
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": "warn",
    "no-console": "off",
    "func-names": "off",
    "no-plusplus": "off",
    "no-process-exit": "off",
    "class-methods-use-this": "off",
    "import/no-anonymous-default-export": "off",
    "no-unused-expressions": "warn",
    "no-unused-labels": "warn",
    "no-undef": "error"
  }
}
